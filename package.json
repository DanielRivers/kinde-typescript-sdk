{
  "name": "@kinde-oss/kinde-typescript-sdk",
  "version": "1.0.0",
  "description": "Kinde Typescript SDK",
  "main": "dist-cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/types",
  "files": ["dist", "dist-cjs"],
  "exports": {
    ".": {
      "require": "./dist-cjs/index.js",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.cjs.json",
    "prebuild": "rimraf dist dist-cjs",
    "dev": "nodemon --exec ts-node ./lib/index.ts"
  },
  "author": {
    "name": "Kinde",
    "email": "engineering@kinde.com",
    "url": "https://kinde.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kinde-oss/kinde-typescript-sdk"
  },
  "bugs": "https://github.com/kinde-oss/kinde-typescript-sdk",
  "homepage": "https://kinde.com",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^20.2.1",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
